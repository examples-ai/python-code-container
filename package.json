{
  "name": "@examples-ai/python-code-container",
  "version": "1.1.0",
  "description": "Python Code Container, wrapping of pyodide",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "node": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "biome check --write src",
    "format": "biome format --write src",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "code-container",
    "python-code-container",
    "container",
    "pyodide",
    "python",
    "typescript",
    "browser",
    "execution",
    "react",
    "hooks"
  ],
  "author": "Jimmy Moon <ragingwind@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@types/node": "^24.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/browser": "^3.2.4",
    "jsdom": "^26.0.0",
    "playwright": "^1.55.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "swr": "^2.3.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pyodide": "^0.28.3"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "swr": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "swr": {
      "optional": true
    }
  }
}
