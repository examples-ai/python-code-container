{
  "name": "code-container-monorepo",
  "version": "1.0.0",
  "description": "Monorepo for code-container packages",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "dev": "pnpm -r run dev",
    "test": "pnpm -r run test",
    "test:browser": "pnpm -r run test:browser",
    "clean": "pnpm -r run clean",
    "typecheck": "pnpm -r run typecheck",
    "build:core": "pnpm --filter @code-container/core run build",
    "build:react": "pnpm --filter @code-container/react run build",
    "test:core": "pnpm --filter @code-container/core run test",
    "test:react": "pnpm --filter @code-container/react run test",
    "dev:examples": "pnpm --filter react-code-container-demo run dev"
  },
  "keywords": [
    "code-container",
    "container",
    "webcontainer",
    "pyodide",
    "python",
    "javascript",
    "typescript",
    "browser",
    "execution",
    "middleware"
  ],
  "author": "Jimmy Moon <ragingwind@gmail.com>",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
